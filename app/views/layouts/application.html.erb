<!DOCTYPE html>
<html>
<head>
  <title>PortalNoticias</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if notice %>
    <div class="alert alert-info" role="alert">
      <%= notice %>
    </div>
  <% end %>
  <div class="row">
    <div class="col-md-12">
      <%= yield %>
    </div>
  </div>
</body>
</html>
